<h1>Insert Nurse</h1>
<form name="Nurse" method="post" action="/">
    <div>
        <label for="Nurse_ID">Nurse_ID</label>
        <input id="Nurse_ID" name="Nurse_ID" type="text" required placeholder="Must start with 2" />
    </div>
    <div>
        <label for="Password">Password</label>
        <input id="Password" name="Password" type="text" required placeholder="Password" />
    </div>
    <div>
        <label for="Name">Name</label>
        <input id="Name" name="Name" type="text" required placeholder="Name" />
    </div>
    <div>
        <label for="Address">Address</label>
        <input id="Address" name="Address" type="text" placeholder="Address" />
    </div>
    <div>
        <label for="Phone_number">Phone_number</label>
        <input id="Phone_number" name="Phone_number" type="text" 
        required placeholder="000-000-0000" />
    </div>
    <div>
        <label for="Medical_Specialty_Department_ID">Medical_Specialty_Department_ID</label>
        <input id="Medical_Specialty_Department_ID" name="Medical_Specialty_Department_ID" type="text" required placeholder="Medical_Specialty_Department_ID" />
    </div>
    <div>
        <input type="submit" value="insert" formaction="/admin/insertNurse" />
    </div>
</form>

